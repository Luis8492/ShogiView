{
  "name": "shogi-kif-viewer",
  "version": "1.0.0",
  "description": "Obsidian plugin to render interactive shogi boards from KIF code blocks.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "ui:serve": "node scripts/ui-serve.mjs",
    "ui:screenshot": "playwright test --config playwright.config.ts"
  },
  "keywords": [],
  "author": "Luis8492",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^16.11.6",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint": "^9.39.1",
    "eslint-plugin-obsidianmd": "^0.1.8",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.8.4"
  }
}
